<script type="text/javascript" charset="utf-8">$(document).ready(function(){
var $grid = $(".container").masonry({
     itemSelector: ".posts",
     initLayout: 1
    });
    $grid.on("layoutComplete", function(o, r) {
     console.log(r.length)
    }), $grid.masonry({
     fitWidth: true,
     horizontalOrder: 1,
     gutter: 120,
     resize: 1
    }), $grid.imagesLoaded().progress(function() {
     $grid.masonry("layout")
    });
});</script>

