<!----- FADE IN NAV ----->

<script>
    window.addEventListener("scroll", function() {
    if (window.scrollY > 300) {
        $('#topnav').fadeIn(500);
    }
    else {
        $('#topnav').fadeOut(500);
    }
},false);
</script>

<!----- SCROLL TO TOP ----->

<script>
    $(document).ready(function(){
    $('#btop').click(function(){
        $('html, body').animate({scrollTop : 0},800);
        return false;
    });

});
</script>

