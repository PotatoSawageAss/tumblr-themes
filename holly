<script type="text/javascript">

/// MASONRY

var $container = $('#container');
    $container.masonry({
        itemSelector: '.posts',
        gutter: 100
    });
    $container.imagesLoaded(function(){
        $container.masonry();
});

/// PHOTOSETS

$(document).ready(function(){
   $('.photo-slideshow').pxuPhotoset({
       lightbox: true,
       rounded: false,
       gutter: '2px',
       borderRadius: '0px',
       photoset: '.photo-slideshow',
       photoWrap: '.photo-data',
       photo: '.pxu-photo'
   });
});

$('img').each(function() {
 var newsrc = $(this).attr('src').replace('gifv','gif');
 $(this).attr('src', newsrc);
});
 
$('a').each(function() {
 var newhref = $(this).attr('href').replace('gifv','gif');
 $(this).attr('href', newhref);
});

/// NOTECOUNT 

$('article').find('.notecount').each(function(){
       var n = $(this).html().split(' ')[0].replace(/,/g, '');
       if (n > 999) {
           n = Math.floor(n / 100) / 10;
           $(this).text(n + 'k n');
       }
   });

/// REMOVE REDIRECTS

$(document).ready(function(){ 
//remove tumblr redirects script by magnusthemes@tumblr 
  $('a[href*="t.umblr.com/redirect"]').each(function(){
      var originalURL = $(this).attr("href").split("?z=")[1].split("&t=")[0];
      var replaceURL = decodeURIComponent(originalURL);
      $(this).attr("href", replaceURL);
  });
});

/// RESPONSIVE VIDEOS

    function flexFrame() {
   $(".caption").each(function() {
        $(this).find("iframe").wrap("<div class='capframe'></div>"); 
        flexibleFrames($(".capframe"));
    });
    flexibleFrames($(".video"));
}
$(document).ready(flexFrame);

/// SCROLL TO TOP
 
$(document).ready(function(){
    $('#btop').click(function(){
        $('html, body').animate({scrollTop : 0},800);
        return false;
    });
 
});

feather.replace()

</script> 

